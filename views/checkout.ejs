    <!-- Checkout Section Begin -->
    <section class="checkout spad">
        <div class="container">
            <div class="checkout__form">
                <!-- <form action="#"> -->
                   
                    <div class="row">
                      
                        <div class="col-lg-8 col-md-6">
                        
                                <div class="cart__discount">
                                    <h6>Discount codes</h6>
                                    
                                        <input id="couponcode" type="text" placeholder="Coupon code">
                                        <button type="button" onclick="Applycoupon(event,<%= locals.subtotal%>,'check')" >Apply</button>
                                        <p id="infopoint"></p>
                                        <button onclick="Applycoupon(event,<%= subtotal%>,'close')" id="close"></button>           
                                </div>
                             <form id="checkout-form">
                            <div class="checkout__order">


                                <h4 class="order__title">Your order</h4>
                                <div class="checkout__order__products">Product <span>Total</span></div>
                                <ul class="checkout__total__products">
                                    
                                <% for(let i=0;i< locals.cartdetails.length;i++) { %>
                                    <li><%= cartdetails[i].Cartdisplayproduct.productname %><span>&#8377 :<%= cartdetails[i].Cartdisplayproduct.offerprice %></span></li>
                                <%}%>
                                    <!-- <li>02. German chocolate <span>&#8377 : 170.</span></li>
                                    <li>03. Sweet autumn <span>&#8377 : 170.0</span></li>
                                    <li>04. Cluten free mini dozen <span>&#8377 : 110.0</span></li> -->
                                </ul>
                               
                                <ul class="checkout__total__all">
                                    <input hidden type="number" name="subtotal"  value="<%= locals.subtotal%>">
                                    <input hidden name="coupon" id="coupon" type="text" value="">
                                    <li>Subtotal <span id="Sub">&#8377 :<%= locals.subtotal%></span></li>
                                    <li>GST(18%):<span id="Gst">&#8377 :<%= Math.round(.18*(subtotal))%></span></li>
                                    <li>Total <span id="Tot">&#8377 :<%= Math.round(1.18*(subtotal))%></span></li>
                                </ul>
                               
                               
                                <%if(locals.userdetails[0].userwallet>0){%>
                                <div class="checkout__input__checkbox">
                                    Wallet: ₹<%= locals.userdetails[0].userwallet%>
                                    <label for="walletyes">
                                        
                                        Use now 
                                        <input required  name="usewallet" type="radio" id="walletyes" value="use">
                                        <span class="checkmark"></span>    
                                    </label>
                                </div>
                                <%if((1.18*locals.subtotal) > locals.userdetails[0].userwallet){%>
                                <div class="checkout__input__checkbox">
                                    <label for="walletno">
                                        Use later
                                        <input required name="usewallet" type="radio" id="walletno" value="dontuse">
                                        <span class="checkmark"></span>    
                                    </label>
                                </div>
                                <%}%>
                                <hr>
                                <%}%>
                                
                                
                                <%if((1.18*locals.subtotal) > locals.userdetails[0].userwallet){%>
                                    <div class="checkout__input__checkbox">
                                        <label for="acc-or">  
                                         <p> Cash on delivery</p>      
                                            <input required name="pay" type="radio" id="acc-or" value="COD">
                                            <span class="checkmark"></span>
                                        </label>
                                    </div>
                                <div class="checkout__input__checkbox">
                                    <label for="payment">
                                        Razorpay
                                        <input required name="pay" type="radio" id="payment" value="Razorpay">
                                        <span class="checkmark"></span>
                                    </label>
                                </div>
                                <div class="checkout__input__checkbox">
                                    <label for="paypal">
                                        Paypal
                                        <input required name="pay" type="radio" id="paypal" value="Paypal">
                                        <span class="checkmark"></span>
                                    </label>
                                </div>
                                <%}
                                else{%> 
                                    <div class="checkout__input__checkbox">
                                        <label for="acc-or">  
                                         <p> Deliver</p>      
                                            <input required name="pay" type="radio" id="acc-or" value="wallet">
                                            <span class="checkmark"></span>
                                        </label>
                                    </div>
                                    <%}%> 
                                <div>
                                   
                                    <ul class="checkout__total__all">
                                        Select an Address:
                                        <hr>  
                                     
                                        <% for(let i=0;i< locals.userdetails[0].useraddress.length;i++) { %>
                                            <label for="">  
                                                <input required name="position" type="radio" id="" value="<%= i %>" >
                                                <!-- <input name="marker" hidden type="number" value="<%= i %>"  > -->
                                                <span class="checkmark">Addesss </span> 
                                                    <p><%= userdetails[0].useraddress[i].name %>,<br><%= userdetails[0].useraddress[i].address %> ,<br><%= userdetails[0].useraddress[i].city %>,<%= userdetails[0].useraddress[i].state %>,<%= userdetails[0].useraddress[i].Country %><br>Pin-<%= userdetails[0].useraddress[i].Pin %>,<br>Phone:<%= userdetails[0].useraddress[i].phone %></p>                   
                                                
                                            </label>   
                                            <%}%>
                                     
                                        <!-- <label for="">
                                            <input name="address" type="radio" id="">
                                            <span class="checkmark">Addesss 2</span> 
                                                <p>vishal varghese jans,<br> koiklaklethu house,churulikodu po,<br>pathanamthitta,kerala,India<br>Pin-689668,<br>Phone:9099090988</p>                   
                                            
                                        </label> -->
                                        <label for="">
                                             <div class="m-5 justify-content-center">
                                             <a href="/addaddresscheckout"> <button type="button" class="btn btn-secondary">+</button></a>
                                        </div>
                                        </label>
                                        <!-- <li>Subtotal <span>&#8377 :<%= locals.subtotal%></span></li>
                                        <li>GST(18%):<span>&#8377 :<%= Math.round(.18*(subtotal))%></span></li>
                                        <li>Total <span>&#8377 :<%= Math.round(1.18*(subtotal))%></span></li> -->
                                    </ul>
                                </div>

                                <button type="submit" class="site-btn">PLACE ORDER</button>

                            </form>                           
                            </div>
                        </div>
                        
                    </div>
                <!-- </form> -->
            </div>
        </div>
    </section>
